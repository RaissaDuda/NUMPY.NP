import numpy as np

array1 = np.arange(1, 11)
array2 = np.arange(2, 21, 2)
array3 = np.array([
    [1, 5, 9],
    [2, 4, 6],
    [7, 8, 3]
])
print(array1)


parte2

import numpy as np

valores = np.array([10, 20, 30, 40, 50])

print(valores[0])
print(valores)
print(valores[-3:])
print(valores)
valores[2] = 35
print(valores)


parte 3
import numpy as np
dados = np.array([12, 45, 67, 23, 89, 56, 33])

print(np.max(dados))
print(np.min(dados))
print(np.sum(dados))
print(np.sort(dados))
